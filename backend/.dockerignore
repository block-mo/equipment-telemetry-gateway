# Dependencies - already handled by multi-stage build, but exclude to reduce context
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Version control - not needed in container
.git
.gitignore
.gitattributes

# IDE and editor files - no need in production image
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store
Thumbs.db

# Environment files - secrets handled separately, not copied into image
.env
.env.local
.env.*.local
.env.test

# Build and test artifacts - not needed in production
dist/
build/
coverage/
.nyc_output/

# Development and CI files
.eslintcache
.prettierignore
.github/
.gitlab-ci.yml
Jenkinsfile

# Documentation and examples - not needed at runtime
README.md
docs/
examples/
CHANGELOG.md
LICENSE

# Docker files - no need to include Dockerfile in context
Dockerfile*
docker-compose*.yaml
.dockerignore

# Test files - tests run in CI, not in production image
tests/
__tests__/
*.test.js
*.spec.js

# Temporary and OS files
tmp/
temp/
*.log
*.pid
*.seed
*.pid.lock

# Node modules cache
.npm/

# Optional npm cache directory
.npm

# Optional eslint cache
.eslintcache
